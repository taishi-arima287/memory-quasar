{
  "name": "memory-quasar",
  "private": true,
  "workspaces": [
    "apps/*",
    "shared"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:web": "npm run dev --workspace=web",
    "dev:admin": "npm run dev --workspace=admin",
    "dev:api": "npm run dev --workspace=api",
    "prisma:studio": "npm run prisma:studio --workspace=api"
  }
}
